{
  "name": "react-space-router",
  "description": "Space router bindings for React.",
  "version": "0.6.1",
  "main": "dist/cjs",
  "module": "dist/esm",
  "license": "ICS",
  "repository": {
    "type": "git",
    "url": "git://github.com/humaans/react-space-router.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": ["react", "browser", "clientside", "router", "history", "location", "url"],
  "author": "Karolis Narkevicius",
  "scripts": {
    "test": "npm run build && healthier && prettier --check '**/*.{js,css,yml}' && c8 ava",
    "format": "prettier --write '**/*.{js,css,yml}'",
    "coverage": "c8 --reporter=html ava",
    "build": "node ./tasks/build.js",
    "watch": "node ./tasks/build.js -w",
    "release": "np --no-release-draft",
    "release:beta": "np --tag=beta --no-release-draft",
    "release:docs": "hugo -s docs && gh-pages -d docs/public"
  },
  "dependencies": {
    "space-router": "^0.9.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.15",
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "execa": "^8.0.1",
    "gh-pages": "^6.0.0",
    "healthier": "^7.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "healthier": {
    "ignore": ["examples"]
  },
  "ava": {
    "files": ["test/*.test.*"],
    "extensions": ["js", "jsx"],
    "require": ["@swc-node/register"]
  }
}
