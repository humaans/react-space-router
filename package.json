{
  "name": "react-space-router",
  "description": "Space router bindings for React.",
  "version": "0.2.0",
  "main": "dist/cjs",
  "module": "dist/esm",
  "license": "ICS",
  "repository": {
    "type": "git",
    "url": "git://github.com/humaans/react-space-router.git"
  },
  "keywords": [
    "react",
    "browser",
    "clientside",
    "router",
    "history",
    "location",
    "url"
  ],
  "author": "Karolis Narkevicius",
  "scripts": {
    "test": "healthier && prettier --check '**/*.{js,css,yml}' && nyc ava",
    "format": "prettier --write '**/*.{js,css,yml}'",
    "coverage": "nyc --reporter=html ava",
    "build": "node ./tasks/build.js",
    "watch": "node ./tasks/build.js -w",
    "version": "npm run build",
    "release": "np --no-release-draft",
    "release:beta": "np --tag=beta --no-release-draft",
    "release:docs": "hugo -s docs && gh-pages -d docs/public"
  },
  "dependencies": {
    "space-router": "^0.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "ava": "^3.15.0",
    "gh-pages": "^3.2.3",
    "healthier": "^4.0.0",
    "jsdom": "^18.0.1",
    "np": "^7.5.0",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "healthier": {
    "ignore": [
      "examples"
    ]
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}
