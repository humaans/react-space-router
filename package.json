{
  "name": "react-space-router",
  "description": "Space router bindings for React.",
  "version": "0.6.4",
  "main": "dist/cjs",
  "module": "dist/esm",
  "license": "ICS",
  "repository": {
    "type": "git",
    "url": "git://github.com/humaans/react-space-router.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "react",
    "browser",
    "clientside",
    "router",
    "history",
    "location",
    "url"
  ],
  "author": "Karolis Narkevicius",
  "scripts": {
    "test": "npm run build && healthier && prettier --check '**/*.{js,css,yml}' && c8 ava",
    "format": "prettier --write '**/*.{js,css,yml}'",
    "coverage": "c8 --reporter=html ava",
    "build": "node ./tasks/build.js",
    "watch": "node ./tasks/build.js -w",
    "release": "np --no-release-draft",
    "release:beta": "np --tag=beta --no-release-draft",
    "release:docs": "hugo -s docs && gh-pages -d docs/public"
  },
  "dependencies": {
    "space-router": "^0.9.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "execa": "^9.6.0",
    "gh-pages": "^6.3.0",
    "healthier": "^7.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "healthier": {
    "ignore": [
      "examples"
    ]
  },
  "ava": {
    "files": [
      "test/*.test.*"
    ],
    "extensions": [
      "js",
      "jsx"
    ],
    "require": [
      "@swc-node/register"
    ]
  }
}
